<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
    <metadata>
        <id>Google.Apis</id>
        <version>VERSION</version>
        <title>Google APIs Client Library</title>
        <authors>Google Inc.</authors>
        <owners>peleyal</owners>
        <licenseUrl>http://www.apache.org/licenses/LICENSE-2.0</licenseUrl>
        <projectUrl>https://code.google.com/p/google-api-dotnet-client/</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>
            The Google APIs Client Library is a runtime client for working with Google Services.
            The library supports service requests, media upload and download, etc.

            Supported Platforms:

            - Windows Store apps

            - Windows Phone 7.5 and 8

            - Silverlight 4 and 5

            - Portable Class Libraries
        </description>
        <tags>Google</tags>
        <dependencies>
            <group>
                <dependency id="Newtonsoft.Json" version="5.0.5" />
                <dependency id="Microsoft.Bcl" version="1.0.19" />
                <dependency id="Microsoft.Bcl.Async" version="1.0.16" />
                <dependency id="Microsoft.Bcl.Build" version="1.0.7" />
                <dependency id="Microsoft.Net.Http" version="2.1.10" />
                <dependency id="Zlib.Portable" version="1.9.2" />
            </group>
            <group targetFramework=".NETFramework4.0">
                <dependency id="Newtonsoft.Json" version="5.0.5" />
                <dependency id="Microsoft.Bcl" version="1.0.19" />
                <dependency id="Microsoft.Bcl.Async" version="1.0.16" />
                <dependency id="Microsoft.Bcl.Build" version="1.0.7" />
                <dependency id="Microsoft.Net.Http" version="2.1.10" />
                <dependency id="Zlib.Portable" version="1.9.2" />
                <dependency id="log4net" version="2.0.0" />
            </group>
        </dependencies>
    </metadata>
